<h1 style="text-align: center;">User List</h1>
<hr>
<br>
{{#if serverSuccess}}
<div class="alert alert-success">{{serverSuccess}}</div>
{{/if}}

{{#if serverError}}
<div class="alert alert-danger">{{serverError}}</div>
{{/if}}

<div class="d-grid gap-2 d-md-flex justify-content-md-start">
 <a href="create" class="btn btn-primary mb-3">Create New User</a>

  
</div>
<table class="table table-striped">
  <thead class="thead-dark">
    <tr>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Mobile</th>
      <th scope="col">Email</th>
      <th scope="col">Actions</th>
    </tr>
  </thead>
  <tbody>
    {{#each userList}}
    <tr>
      <th scope="row">{{this.first_name}}</th>
      <th scope="row">{{this.last_name}}</th>
      <th scope="row">{{this.mobile}}</th>
      <th scope="row">{{this.email}}</th>
      <td>
        <a href="/edit/{{this.id}}" class="btn btn-primary">Edit</a>
        <a href="/delete/{{this.id}}" class="btn btn-danger">Delete</a>
      </td>
    </tr>
    {{/each}}
    
  </tbody>
</table>